<h1>ArticleTypes#index</h1>
<p>Find me in app/views/administration/landing/index.html.erb</p>
<%= link_to "Sign out", administration.destroy_admin_session_path, method: :delete %>
<table>
  <% @types.each do |type| %>
    <tr>
      <td><%= type.title %></td>
      <td><%= link_to "Edit", administration.edit_article_type_path(type.id) %></td>
      <td><%= link_to 'Destroy', administration.article_type_path(type.id), confirm: 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>