<h1>Administration::Publications#index</h1>
<table>
  <% @publications.each do |article| %>
    <tr>
      <td><%= link_to article.title, administration.publication_path(article.id) %></td>
      <td><%= link_to "Edit", administration.edit_publication_path(article.id) %></td>
      <td>
        <%= button_to "Delete", administration.publication_path(article.id), data: { confirm: "Are you sure?" }, method: :delete %>
      </td>
    </tr>
  <% end %>
</table>